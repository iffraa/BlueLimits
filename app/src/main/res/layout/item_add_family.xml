<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/_10sdp">


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <TextView
                android:layout_width="@dimen/_75sdp"
                android:layout_height="@dimen/_35sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:fontFamily="@font/jura_demi_bold"
                android:gravity="center_vertical"
                android:text="@string/first_name"
                android:textSize="@dimen/_14ssp"
                android:textColor="@color/navy_blue"></TextView>

            <TextView
                android:layout_width="@dimen/_75sdp"
                android:layout_height="@dimen/_35sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:textColor="@color/navy_blue"
                android:fontFamily="@font/jura_demi_bold"
                android:gravity="center_vertical"
                android:textSize="@dimen/_14ssp"
                android:text="@string/last_name"></TextView>


            <TextView
                android:layout_width="@dimen/_75sdp"
                android:layout_height="@dimen/_35sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:textColor="@color/navy_blue"
                android:fontFamily="@font/jura_demi_bold"
                android:gravity="center_vertical"
                android:textSize="@dimen/_14ssp"
                android:text="@string/email"></TextView>

            <TextView
                android:layout_width="@dimen/_75sdp"
                android:layout_height="@dimen/_35sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:textColor="@color/navy_blue"
                android:fontFamily="@font/jura_demi_bold"
                android:gravity="center_vertical"
                android:textSize="@dimen/_14ssp"
                android:text="@string/id_profile"></TextView>

            <TextView
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_35sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:textColor="@color/navy_blue"
                android:fontFamily="@font/jura_demi_bold"
                android:gravity="center_vertical"
                android:textSize="@dimen/_14ssp"
                android:text="@string/mobile"></TextView>

            <TextView
                android:layout_width="@dimen/_75sdp"
                android:layout_height="@dimen/_35sdp"
                android:layout_marginTop="@dimen/_17sdp"
                android:textColor="@color/navy_blue"
                android:fontFamily="@font/jura_demi_bold"
                android:gravity="center_vertical"
                android:textSize="@dimen/_14ssp"
                android:text="@string/dob"></TextView>


            <TextView
                android:layout_width="@dimen/_75sdp"
                android:layout_height="@dimen/_35sdp"
                android:layout_marginTop="@dimen/_12sdp"
                android:textColor="@color/navy_blue"
                android:textSize="@dimen/_14ssp"
                android:fontFamily="@font/jura_demi_bold"
                android:gravity="center_vertical"
                android:text="@string/gender"></TextView>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <EditText
                android:id="@+id/et_fname"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_35sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:textColor="@color/navy_blue"
                android:background="@drawable/rectangle"
                android:fontFamily="@font/jura_demi_bold"
                android:inputType="text"
                android:paddingLeft="@dimen/_10sdp"
                android:textSize="@dimen/_11ssp"
                android:lines="1"></EditText>

            <EditText
                android:id="@+id/et_lname"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_35sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:textColor="@color/navy_blue"
                android:textSize="@dimen/_11ssp"
                android:paddingLeft="@dimen/_10sdp"
                android:background="@drawable/rectangle"
                android:fontFamily="@font/jura_demi_bold"
                android:inputType="text"
                android:lines="1"></EditText>

            <EditText
                android:id="@+id/et_email"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_35sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:textColor="@color/navy_blue"
                android:textSize="@dimen/_11ssp"
                android:paddingLeft="@dimen/_10sdp"
                android:background="@drawable/rectangle"
                android:fontFamily="@font/jura_demi_bold"
                android:inputType="textEmailAddress"
                android:lines="1"></EditText>


            <EditText
                android:id="@+id/et_id"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_35sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:textColor="@color/navy_blue"
                android:background="@drawable/rectangle"
                android:fontFamily="@font/jura_demi_bold"
                android:textSize="@dimen/_11ssp"
                android:paddingLeft="@dimen/_10sdp"
                android:inputType="number"
                android:maxLength="10"
                android:lines="1"></EditText>

            <include
                android:id="@+id/layout_mobile"
                layout="@layout/layout_contact_number"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40sdp"
                android:layout_marginTop="@dimen/_15sdp"></include>

            <EditText
                android:id="@+id/et_dob"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_35sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:background="@drawable/rectangle"
                android:fontFamily="@font/jura_demi_bold"
                android:focusableInTouchMode="false"
                android:textSize="@dimen/_11ssp"
                android:paddingLeft="@dimen/_10sdp"
                android:textColor="@color/navy_blue"
                android:lines="1"></EditText>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_15sdp"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/checkbox_male"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/jura_demi_bold"
                    android:text="@string/male"
                    android:button="@drawable/chkbox_bg"
                    android:paddingLeft="@dimen/_10sdp"
                    android:buttonTint="@color/grey_text"
                    android:checked="true"
                    android:textSize="@dimen/_11ssp"
                    android:textColor="@color/navy_blue" />
s
                <CheckBox
                    android:id="@+id/checkbox_female"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/chkbox_bg"
                    android:paddingLeft="@dimen/_10sdp"
                    android:fontFamily="@font/jura_demi_bold"
                    android:text="@string/female"
                    android:layout_marginLeft="@dimen/_30sdp"
                    android:buttonTint="@color/grey_text"
                    android:textSize="@dimen/_11ssp"
                    android:textColor="@color/navy_blue" />
            </LinearLayout>


        </LinearLayout>


</LinearLayout>